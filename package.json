{
    "type": "module",
    "version": "1.0.12",
    "author": "BlobMaster41",
    "description": "The perfect library for building Bitcoin based applications.",
    "engines": {
        "node": ">=16.0.0"
    },
    "exports": {
        "node": {
            "import": "./build/index.js",
            "require": "./cjs/index.js",
            "types": "./build/index.d.ts"
        },
        "browser": {
            "import": "./browser/index.js",
            "require": "./browser/index.js",
            "types": "./browser/index.d.ts"
        }
    },
    "browser": {
        "Buffer": "buffer"
    },
    "homepage": "https://opnet.org",
    "keywords": [
        "bitcoin",
        "opnet",
        "btc",
        "bitcoin smart contracts",
        "smart inscriptions",
        "bsi"
    ],
    "license": "MIT",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "typings": "build/index.d.ts",
    "module": "build/index.js",
    "name": "opnet",
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/bsi-vision/opnet.git"
    },
    "scripts": {
        "watch": "gulp watch",
        "build": "gulp",
        "test": "jest --runInBand",
        "setup": "npm npm i && npm run build",
        "browserBuild": "webpack --mode production",
        "cjs": "gulp cjs",
        "docs": "typedoc --out docs --tsconfig tsconfig.json --readme README.md --name OPNet --plugin typedoc-material-theme --themeColor '#cb9820' --exclude src/scripts/test.ts --exclude src/index.ts src",
        "install": "gulp && gulp cjs"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/preset-env": "^7.24.5",
        "@babel/preset-flow": "^7.24.1",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@types/node": "^20.11.30",
        "@types/ws": "^8.5.10",
        "assert": "^2.1.0",
        "crypto-browserify": "^3.12.0",
        "eslint": "^8.57.0",
        "https-browserify": "^1.0.0",
        "jest": "^29.7.0",
        "os-browserify": "^0.3.0",
        "prettier": "^3.2.5",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "typedoc": "^0.25.13",
        "typedoc-material-theme": "^1.0.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.4.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@bitcoinerlab/secp256k1": "^1.1.1",
        "@btc-vision/bsi-binary": "github:btc-vision/bsi-binary",
        "@btc-vision/bsi-bitcoin-rpc": "github:btc-vision/bsi-bitcoin-rpc",
        "@jest/types": "^29.6.3",
        "@types/jest": "^29.5.12",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "babel-preset-react": "^6.24.1",
        "babelify": "^10.0.0",
        "bitcoinjs-lib": "^6.1.5",
        "buffer": "^6.0.3",
        "crypto": "^1.0.1",
        "ethers": "^6.12.1",
        "gulp": "^4.0.2",
        "gulp-cached": "^1.1.1",
        "gulp-logger": "^0.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "node-polyfill-webpack-plugin": "^3.0.0",
        "stream": "^0.0.2",
        "tiny-secp256k1": "^2.2.3",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2"
    }
}
